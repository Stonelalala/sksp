  .other-stone-box image{width: 60rpx;height: 60rpx;}
  .other-stone-box .stone-content{font-size: 26rpx; margin:0 20rpx; }
  .other-stone-box:before{
  border: 20rpx solid transparent;
  border-top: 20rpx solid #ccc;
  content: "";
  position: absolute;
  top: 100%;
  left: 78%;
  width: 0;
  height: 0;
}
 .other-stone-box:after{
  border: 20rpx solid transparent;
  border-top: 20rpx solid #fff;
  content: "";
  position: absolute;
  top: 100%;
  left: 78%;
  margin-top:-8rpx; 
  width: 0;
  height: 0;
}


